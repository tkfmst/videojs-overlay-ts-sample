{
  "scripts": {
    "build": "webpack --progress --colors --config webpack.prod.js",
    "build-dev": "webpack --progress --colors --config webpack.dev.js",
    "server": "(yarn build-dev) && (webpack-dev-server --config webpack.dev.js --inline --content-base . --watch-content-base --open-page index.html)",
    "server-only": "webpack-dev-server --config webpack.dev.js --inline --content-base . --watch-content-base --open-page index.html",
    "lint": "eslint src/ts --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest src/ts/test # 結果がfile単位",
    "test-detail": "find src/ts/test -name '*.test.ts' | xargs -I% yarn jest %",
    "check-ts": "(yarn lint) && (yarn tsc --noEmit) && (yarn test)",
    "check-detail-ts": "(yarn lint) && (yarn tsc --noEmit) && (yarn test-detail)",
    "c": "yarn check-ts",
    "cd": "yarn check-detail-ts"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "video.js": "^7.8.2",
    "videojs-overlay": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@types/jest": "^25.1.2",
    "@types/video.js": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-loader": "^8.0.6",
    "babel-preset-power-assert": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "html-loader": "^1.0.0",
    "html-minifier-terser": "^5.0.4",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "prettier": "^2.0.1",
    "regenerator-runtime": "^0.13.3",
    "style-loader": "^1.2.1",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsc": "^1.20150623.0",
    "typedoc": "^0.17.2",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "workspaces": [
    "src/ts/main/packages/common",
    "src/ts/main/packages/entity",
    "src/ts/main/packages/usecase",
    "src/ts/main/packages/interface",
    "src/ts/main/packages/infrastructure"
  ],
  "private": true
}
